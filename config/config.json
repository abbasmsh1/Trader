{
  "app_name": "Crypto Trader",
  "version": "0.1.0",
  "testnet": true,
  "demo_mode": true,
  "update_interval": 60,
  "initial_balance": 20.0,
  "symbols": ["BTC/USDT", "ETH/USDT", "SOL/USDT", "BNB/USDT"],
  "min_trade_value": 5.0,
  "database": {
    "type": "pickle",
    "path": "models"
  },
  "agents": {
    "system_controller": {
      "name": "System Controller",
      "enabled": true
    },
    "market_analyzer": [
      {
        "name": "BTC Market Analyzer",
        "enabled": true,
        "symbols": ["BTC/USDT"],
        "indicators": ["rsi", "macd", "bollinger"]
      },
      {
        "name": "ETH Market Analyzer",
        "enabled": true,
        "symbols": ["ETH/USDT"],
        "indicators": ["rsi", "macd", "bollinger"]
      },
      {
        "name": "SOL Market Analyzer",
        "enabled": true,
        "symbols": ["SOL/USDT"],
        "indicators": ["rsi", "macd", "bollinger"]
      }
    ],
    "strategy_agents": [
      {
        "name": "Trend Following Strategy",
        "type": "trend_strategy",
        "enabled": true,
        "config": {
          "trend_threshold": 0.05,
          "entry_signal_threshold": 0.6,
          "exit_signal_threshold": 0.4,
          "max_positions": 3
        },
        "dependencies": ["market_analyzer"]
      },
      {
        "name": "Swing Trading Strategy",
        "type": "swing_strategy",
        "enabled": true,
        "config": {
          "swing_threshold": 0.03,
          "max_swing_duration": 14,
          "entry_signal_threshold": 0.7,
          "exit_signal_threshold": 0.3
        },
        "dependencies": ["market_analyzer"]
      },
      {
        "name": "Breakout Strategy",
        "type": "breakout_strategy",
        "enabled": true,
        "config": {
          "breakout_threshold": 0.1,
          "confirmation_period": 3,
          "false_breakout_filter": true
        },
        "dependencies": ["market_analyzer"]
      }
    ],
    "portfolio_manager": {
      "name": "Portfolio Manager",
      "enabled": true,
      "config": {
        "max_allocation_per_position": 0.25,
        "min_cash_reserve": 0.1,
        "rebalance_threshold": 0.1,
        "risk_factor": 0.5
      },
      "dependencies": ["strategy_agents"]
    },
    "execution_agent": {
      "name": "Execution Agent",
      "enabled": true,
      "config": {
        "slippage_model": "percentage",
        "slippage_factor": 0.002,
        "max_retries": 3
      }
    },
    "risk_manager": {
      "name": "Risk Manager",
      "enabled": true,
      "config": {
        "max_drawdown": 0.15,
        "position_size_limit": 0.2,
        "stop_loss_percentage": 0.05,
        "trailing_stop": true
      },
      "dependencies": ["portfolio_manager"]
    },
    "trader_agents": [
      {
        "name": "Buffett Trader",
        "type": "buffett_trader",
        "enabled": true,
        "description": "Value investor focused on fundamentals",
        "trading_style": "value",
        "config": {
          "min_holding_period": 30,
          "margin_of_safety": 0.3,
          "max_position_size": 0.25,
          "fundamental_weight": 0.8,
          "technical_weight": 0.2
        }
      },
      {
        "name": "Soros Trader",
        "type": "soros_trader",
        "enabled": true,
        "description": "Macro investor focused on reflexivity",
        "trading_style": "macro",
        "config": {
          "reflexivity_threshold": 0.15,
          "cycle_periods": [7, 14, 30],
          "sentiment_weight": 0.4,
          "leverage_multiplier": 1.5,
          "regime_threshold": 0.2
        }
      },
      {
        "name": "Simons Trader",
        "type": "simons_trader",
        "enabled": true,
        "description": "Quantitative trader using statistical models",
        "trading_style": "quant",
        "config": {
          "mean_reversion_periods": [3, 5, 10],
          "zscore_threshold": 2.0,
          "correlation_threshold": 0.7,
          "volatility_scaling": true,
          "max_position_duration": 5,
          "statistical_confidence": 0.95
        }
      },
      {
        "name": "Lynch Trader",
        "type": "lynch_trader",
        "enabled": true,
        "description": "Growth investor looking for tenbaggers",
        "trading_style": "growth",
        "config": {
          "growth_periods": [7, 14, 30],
          "growth_threshold": 0.1,
          "pattern_confidence": 0.8,
          "momentum_weight": 0.6,
          "max_holding_period": 60,
          "ten_bagger_threshold": 0.15
        }
      }
    ],
    "historical_analyzer": {
      "enabled": true,
      "name": "HistoricalAnalyzer",
      "description": "Analyzes historical market data and provides insights",
      "timeframes": ["1h", "4h", "1d"],
      "lookback_periods": {
        "1h": 30,
        "4h": 30,
        "1d": 30
      },
      "symbols": ["BTC/USDT", "ETH/USDT", "BNB/USDT", "ADA/USDT", "DOGE/USDT"],
      "analysis_interval": 3600
    },
    "market_data_service": {
      "enabled": true,
      "api_key": "YOUR_API_KEY",
      "secret": "YOUR_API_SECRET"
    },
    "execution_agent": {
      "enabled": true,
      "name": "ExecutionAgent",
      "description": "Executes trades in the market",
      "max_retries": 3,
      "retry_delay": 5,
      "slippage_model": "linear",
      "min_order_size": 10.0
    },
    "trader_agents": [
      {
        "enabled": true,
        "type": "value_trader",
        "name": "ValueTrader",
        "description": "Value investing focused trader",
        "symbols": ["BTC/USDT", "ETH/USDT", "BNB/USDT"],
        "position_size": 0.1,
        "max_positions": 3,
        "stop_loss": 0.05,
        "take_profit": 0.15
      },
      {
        "enabled": true,
        "type": "momentum_trader",
        "name": "MomentumTrader",
        "description": "Momentum and trend following trader",
        "symbols": ["BTC/USDT", "ETH/USDT", "ADA/USDT", "DOGE/USDT"],
        "position_size": 0.15,
        "max_positions": 4,
        "stop_loss": 0.03,
        "take_profit": 0.1
      },
      {
        "enabled": true,
        "type": "contrarian_trader",
        "name": "ContrarianTrader",
        "description": "Contrarian trading strategy",
        "symbols": ["BTC/USDT", "ETH/USDT", "BNB/USDT"],
        "position_size": 0.1,
        "max_positions": 3,
        "stop_loss": 0.07,
        "take_profit": 0.2
      }
    ],
    "optimizer": {
      "enabled": true,
      "name": "OptimizerAgent",
      "description": "Optimizes trading strategy parameters",
      "optimization_method": "genetic",
      "population_size": 50,
      "generations": 20,
      "mutation_rate": 0.2,
      "crossover_rate": 0.7,
      "grid_search_steps": 5,
      "metrics": ["sharpe_ratio", "max_drawdown", "profit_factor", "win_rate"],
      "api_key": "YOUR_API_KEY",
      "secret": "YOUR_API_SECRET"
    }
  }
} 